{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "func-names": "off",
    "arrow-body-style": "off",
    "object-shorthand": "off",
    "prefer-destructuring": "off"
  }
}