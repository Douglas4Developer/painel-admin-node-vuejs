{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "vitest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "vue/no-multiple-template-root": "off"
  }
}